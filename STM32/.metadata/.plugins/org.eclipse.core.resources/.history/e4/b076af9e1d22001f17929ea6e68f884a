#ifndef TRAJ_H_
#define TRAJ_H_

#define MAX_ROWS 1000
#define COLS 8

// 定义一个结构体来存储 CSV 解析后的数据
typedef struct {
    int data[MAX_ROWS][COLS];
    int rows;
} CSVData;

void Start_Local_Traj(unsigned char traj_type);
void Start_Global_Traj(unsigned char traj_type);

#endif /* TRAJ_H_ */
